#: pigeonplanner/ui/widgets/bandentry.py:38
msgid "Invalid input!"
msgstr "Pogrešan unos"

#: pigeonplanner/messages.py:87
msgid "The ringnumber and year are necessary."
msgstr "Broj prstena i godina su obavezni."

#: pigeonplanner/messages.py:90 pigeonplanner/ui/widgets/bandentry.py:232
msgid "Only numbers are accepted as year input."
msgstr "Za upis godine prihvaćaju se samo brojevi."

#: pigeonplanner/messages.py:93
msgid "Check the length of the year."
msgstr "Provjeri dužinu godine."

#: pigeonplanner/messages.py:96
msgid "Only numbers are allowed when adding multiple pigeons."
msgstr "Dopušteni su samo brojevi kad se dodaje više golubova."

#: pigeonplanner/messages.py:100
msgid "At least the date and racepoint have to be entered."
msgstr "Moraju biti upisani barem nadnevak i mjesto puštanja."

#: pigeonplanner/messages.py:103
msgid ""
"The date you entered has the wrong format. It should be ISO-format (YYYY-MM-"
"DD)."
msgstr ""
"Upisani nadnevak u neispravnom je formatu. Mora biti upisan u ISO formatu "
"(GGGG-MM-DD)."

#: pigeonplanner/messages.py:107
msgid "The result you want to add already exists."
msgstr "Rezultat koji želiš dodati već postoji."

#: pigeonplanner/messages.py:109
msgid "Removing the selected result"
msgstr "Uklanjanje izabranog rezultata"

#: pigeonplanner/messages.py:110 pigeonplanner/messages.py:113
#: pigeonplanner/messages.py:116 pigeonplanner/messages.py:157
#: pigeonplanner/ui/tabs/medication.py:112
#: pigeonplanner/ui/tools/racepointsmap.py:140
msgid "Are you sure?"
msgstr "Jeste li sigurni?"

#: pigeonplanner/messages.py:112
msgid "Removing the selected breeding record"
msgstr "Uklanjanje odabranog uzgojnog zapisa"

#: pigeonplanner/messages.py:115
msgid "Removing the selected media file"
msgstr "Uklanjanje odabrane medijske datoteke"

#: pigeonplanner/messages.py:120
msgid "The name has to be entered."
msgstr "Ime mora biti upisano."

#: pigeonplanner/messages.py:123
msgid "The persoon you want to add already exists."
msgstr "Osoba koju želiš dodati već postoji."

#: pigeonplanner/messages.py:125
#, python-format
msgid "Removing '%s'"
msgstr "Uklanjanje '%s'"

#: pigeonplanner/messages.py:126
msgid "Are you sure you want to remove this person from your addresses?"
msgstr "Jesi li siguran da želiš ukloniti ovu osobu iz svog adresara?"

#: pigeonplanner/messages.py:130
#, python-format
msgid "Removing '%s' from '%s'."
msgstr "Uklanjanje '% s' iz '% s'."

#: pigeonplanner/messages.py:131
msgid "Are you sure you want to remove this item?"
msgstr "Jesi li siguran da želiš ukloniti ovu stavku?"

#: pigeonplanner/messages.py:134
msgid "The backup was successfully created."
msgstr "Sigurnosna kopija je kreirana."

#: pigeonplanner/messages.py:136 pigeonplanner/messages.py:139
msgid "Completed!"
msgstr "Gotovo!"

#: pigeonplanner/messages.py:137
msgid "The backup was successfully restored."
msgstr "Sigurnosna kopija uspješno je vraćena."

#: pigeonplanner/messages.py:138
msgid "The program will now quit automatically to apply all changes."
msgstr "Program će se sada automatski završiti i primijeniti sve promjene."

#: pigeonplanner/messages.py:140
msgid "No database selected to restore."
msgstr "Nije odabrana baza podataka za vraćanje."

#: pigeonplanner/messages.py:141
msgid "Do you want to continue anyway?"
msgstr "Želite li svejedno nastaviti?"

#: pigeonplanner/messages.py:144
msgid "No personal information found."
msgstr "Nisu pronađene osobne informacije."

#: pigeonplanner/messages.py:145
msgid ""
"This will be shown on top of the printed pedigree.\n"
"Do you want to add it now?"
msgstr ""
"Ovo će se prikazati na vrhu ispisanog rodovnika. \n"
"Želiš li to dodati sada?"

#: pigeonplanner/messages.py:149
msgid "Error printing the pedigree"
msgstr "Pogreška prilikom ispisa rodovnika"

#: pigeonplanner/messages.py:152
msgid "An error occured in the print operation!"
msgstr "Došlo je do pogreške u ispisu!"

#: pigeonplanner/messages.py:153
msgid "See the logfile (File->Logfile viewer) for more info."
msgstr ""
"Pogledaj log datoteku (File->Logfile preglednik) za više informacija."

#: pigeonplanner/messages.py:156
msgid "This will set back all the settings to the default values."
msgstr "Ovo će se sve postavke vratiti na zadane vrijednosti."

#: pigeonplanner/messages.py:158
msgid "Default settings"
msgstr "Uobičajene postavke"

#: pigeonplanner/messages.py:160
msgid "Pigeon Planner needs to be restarted for the changes to take effect."
msgstr ""
"Pigeon Planner mora se ponovo pokrenuti da bi učinak promjena bio vidljiv."

#: pigeonplanner/messages.py:163
msgid "Restart required"
msgstr "Potrebno je ponovno pokretanje"

#: pigeonplanner/messages.py:165
msgid "Update available!"
msgstr "Dostupna je nadogradnja!"

#: pigeonplanner/messages.py:166
msgid "There is an update available, do you want to download it now?"
msgstr "Dostupna je nadogradnja, želiš li sada pokrenuti download?"

#: pigeonplanner/messages.py:169
msgid "The recipient and sender's e-mailaddress are required."
msgstr "E-adrese pošiljatelja i primatelja su obavezne."

#: pigeonplanner/messages.py:173
msgid "Error trying to get information. Are you connected to the internet?"
msgstr "Pogreška pri dohvatu podataka. Jesi li spojen na internet?"

#: pigeonplanner/messages.py:174
msgid "A new version is available."
msgstr "Nova inačica je dostupna."

#: pigeonplanner/messages.py:175
msgid "You already have the latest version installed."
msgstr "Već imaš instaliranu posljednju inačicu."

#: pigeonplanner/messages.py:176
msgid "This isn't normal, or you must be running a development version"
msgstr "Ovo nije uobičajeno, vjerovatno ste pokrenuli razvojnu inačicu"

